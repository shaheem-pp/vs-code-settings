{
	"bracket-pair-colorizer-2.depreciation-notice": false,
	"code-runner.runInTerminal": true,
	"debug.console.wordWrap": true,
	"diffEditor.ignoreTrimWhitespace": false,
	"diffEditor.wordWrap": "on",
	"editor.accessibilitySupport": "off",
	"editor.cursorBlinking": "smooth",
	"editor.cursorSmoothCaretAnimation": true,
	"editor.cursorStyle": "line",
	"editor.detectIndentation": false,
	"editor.fontFamily": "'JetBrains Mono', monospace",
	"editor.fontLigatures": true,
	"editor.formatOnPaste": true,
	"editor.formatOnSave": true,
	"editor.formatOnSaveMode": "file",
	"editor.formatOnType": true,
	"editor.inlineSuggest.enabled": true,
	"editor.insertSpaces": false,
	"editor.linkedEditing": true,
	"editor.suggestSelection": "first",
	"editor.tabSize": 4,
	"editor.wordWrap": "on",
	"files.autoSave": "afterDelay",
	"files.exclude": {
		"**/.project": true,
		"**/.settings": true,
		"**/.classpath": true,
		"**/.factorypath": true
	},
	"liveServer.settings.donotVerifyTags": true,
	"prettier.tabWidth": 2,
	"prettier.useTabs": true,
	"python.formatting.provider": "black",
	"terminal.integrated.cursorBlinking": true,
	"terminal.integrated.cursorStyle": "line",
	"terminal.integrated.defaultProfile.windows": "Command Prompt",
	"terminal.integrated.env.osx": {
		"FIG_NEW_SESSION": "1"
	},
	"terminal.integrated.fontFamily": "MesloLGS NF",
	"terminal.integrated.sendKeybindingsToShell": true,
	"terminal.integrated.tabs.enabled": true,
	"window.title": "${dirty}${appName}${separator}${activeEditorLong}",
	"workbench.colorCustomizations": {
		"terminal.ansiWhite": "#ABB2BF",
		"terminal.ansiBrightCyan": "#56B6C2",
		"terminalCursor.background": "#ABB2BF",
		"terminal.ansiYellow": "#E5C07B",
		"terminal.ansiCyan": "#56B6C2",
		"terminal.ansiBrightYellow": "#E5C07B",
		"terminal.ansiBrightGreen": "#98C379",
		"terminalCursor.foreground": "#ABB2BF",
		"terminal.background": "#202428",
		"terminal.ansiBrightBlack": "#545862",
		"terminal.ansiBrightMagenta": "#C678DD",
		"terminal.ansiMagenta": "#C678DD",
		"[Material Theme Darker High Contrast]": {},
		"terminal.ansiBrightRed": "#E06C75",
		"terminal.ansiRed": "#E06C75",
		"terminal.foreground": "#ABB2BF",
		"terminal.ansiGreen": "#98C379",
		"terminal.ansiBlue": "#61AFEF",
		"terminal.ansiBrightBlue": "#61AFEF",
		"terminal.ansiBrightWhite": "#C8CCD4",
		"terminal.ansiBlack": "#1d1d20"
	},
	"workbench.colorTheme": "GitHub Dark",
	"workbench.editor.enablePreview": false,
	"workbench.editor.openPositioning": "right",
	"workbench.editor.pinnedTabSizing": "compact",
	"workbench.editor.tabSizing": "fit",
	"workbench.editor.wrapTabs": true,
	"workbench.iconTheme": "material-icon-theme",
	"[jsonc]": {
		"editor.defaultFormatter": "vscode.json-language-features"
	},
	"workbench.productIconTheme": "material-product-icons"
}
